<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >

<mapper namespace="Spu">

    <select id="findByCategoryIdAndSpuCode" parameterType="map" resultMap="SpuMap">
        SELECT * FROM
        <include refid="tb"/>
        WHERE category_id=#{categoryId} AND spu_code=#{spuCode}
    </select>
</mapper>
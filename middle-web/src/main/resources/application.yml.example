logging:
  file: logs/middle-web.log

spring:
  datasource:
    driver-class-name: com.mysql.jdbc.Driver
    url: jdbc:mysql://127.0.0.1:3306/pousheng?useUnicode=true&characterEncoding=UTF8
    username: root
    password: anywhere

mybatis:
  mapper-locations: classpath*:mapper/*Mapper.xml
  type-aliases-package: >
      com.pousheng.(erp|auth).model,
      com.pousheng.middle.warehouse.model,
      com.pousheng.middle.order.model,
      io.terminus.parana.(item|category|brand|spu|shop).model,
      io.terminus.parana.(order|cart|express|promotion|delivery|settle|auth).model,
      io.terminus.open.client.common.(mappings|token|shop).model,
      io.terminus.open.client.item.model



session:
  cookie-context-path: /
  cookie-domain: middle-dev.pousheng.com
  cookie-name: msid
  cookie-max-age: 1800
  source: redis
  serialize-type: json
  redis-host: 127.0.0.1
  redis-port: 6379
  redis-index: 0
  redis-cluster: false
  redis-test-on-borrow: true
  redis-max-total: 10
  redis-max-idle: 0
  redis-prefix: afsession



redis:
  host: 127.0.0.1
  port: 6379
  max-total: 10
  max-idle: 2
  test-on-borrow: true


